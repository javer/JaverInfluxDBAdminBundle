{
    "name": "javer/influxdb-admin-bundle",
    "type": "symfony-bundle",
    "description": "Provides integration of InfluxDB ODM with SonataAdminBundle",
    "keywords": ["influxdb", "odm", "sonata", "admin", "symfony"],
    "homepage": "https://github.com/javer/JaverInfluxDBAdminBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Vadim Borodavko",
            "email": "vadim.borodavko@gmail.com",
            "homepage": "https://github.com/javer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "javer/influxdb-odm": "^1.0.4",
        "javer/influxdb-odm-bundle": "^1.0",
        "sonata-project/admin-bundle": "^4.17",
        "sonata-project/form-extensions": "^1.15 || ^2.0",
        "symfony/config": "^5.4 || ^6.0 || ^7.0",
        "symfony/dependency-injection": "^5.4 || ^6.0 || ^7.0",
        "symfony/form": "^5.4 || ^6.0 || ^7.0",
        "symfony/http-kernel": "^5.4 || ^6.0 || ^7.0",
        "symfony/yaml": "^5.4 || ^6.0 || ^7.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.0",
        "squizlabs/php_codesniffer": "3.8.*",
        "swivl/php-coding-standard": "^1.4"
    },
    "conflict": {
        "sonata-project/exporter": "<2.8.0"
    },
    "autoload": {
        "psr-4": {
            "Javer\\InfluxDB\\AdminBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
